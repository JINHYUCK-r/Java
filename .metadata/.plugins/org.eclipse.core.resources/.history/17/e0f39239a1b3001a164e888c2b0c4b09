package com.practice;



public class Practice1 {
	public static void main(String[] args) {
		int[] arr = new int[3];
		int rval=(int)(Math.random()*9+1);
		
		for(int i=0; i<arr.length-1; i++) {
			if(arr[i]==0) {
				arr[i]= rval;
			}else if(arr[i]==arr[i+1]) {
				i--;
			}
		}
		for(int a : arr) {
			System.out.println(a);
		}
	
		
	}
	
}
