package com.practice;



public class Practice1 {
	public static void main(String[] args) {
		int[] arr = new int[3];
	
		
		
		for(int i=0; i<arr.length; i++) {
			int rVal = (int)(Math.random()*9+1);	
		
			for(int z=0; z<arr.length; z++) {			
				if(arr[z]==0) {
					arr[z]=rVal;
				} else if(arr[z]== rVal) {
					i--;
				}
			}
		}
	
		for(int a : arr) {
			System.out.println(a);
		}
	
		
	}
	
}
