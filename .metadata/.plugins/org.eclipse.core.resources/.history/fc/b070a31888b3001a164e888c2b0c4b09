package com.Class;

public class Class1 {

	public static void main(String[] args) {
		int[] arr = new int[3];
		int[] arr2= new int[10];
		String[] arr3= new String[5];
		
		arr[0]=3; // 값넣기
		
		System.out.println(arr2[0]);	//0
		System.out.println(arr3[0]);	// null
		
		String[] arr4= {"a","b","c","d","e"};
		
		for(int i=0; i<10; i++) {
			arr2[i]=i+1;
		}
		for(int k : arr2) {
			System.out.println(k);
		}
		
		
		
	}

}
